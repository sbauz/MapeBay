Package: MapeBay
Type: Package
Title: Multivariate Analysis of Variance Panel, PERMANOVA and Bayesian
Version: 0.1.0
Authors@R: c(person("Maria", "Mendoza-Marcillo", role = c("aut", "cre"),
                     email = "mtmendoz@espol.edu.ec"),
              person("Johny", "Pambabay-Calero", role = c("aut","ctb"),
